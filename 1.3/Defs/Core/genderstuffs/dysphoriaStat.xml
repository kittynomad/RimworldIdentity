<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <StatDef>
    <defName>dysphoriaStat</defName>
    <label>Dysphoria (ungendered)</label>
    <description>The level of satisfaction one has with their body, affected by body/voice related traits. Tracks ungendered factors only; gendered factors have different effects based on what a pawn identifies as.</description>
    <category>BasicsPawn</category>
    <defaultBaseValue>0</defaultBaseValue>
    <minValue>-100</minValue>
    <maxValue>100</maxValue>
    <roundValue>false</roundValue>
    <toStringStyle>FloatTwo</toStringStyle>
    <formatString>{0} %</formatString>
    <roundToFiveOver>200</roundToFiveOver>
    <showNonAbstract>true</showNonAbstract>
    <scenarioRandomizable>true</scenarioRandomizable>
    <showOnPawns>true</showOnPawns>
    <showOnAnimals>false</showOnAnimals>
    <showOnMechanoids>false</showOnMechanoids>
    <displayPriorityInCategory>99997</displayPriorityInCategory>
    
  </StatDef>
  
